# SQL REST API Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DB_SERVER=your_server_address
DB_NAME=AdventureWorks2022
DB_USER=your_database_user
DB_PASSWORD=your_database_password
DB_TRUST_CERT=true

# JWT Configuration
JWT_KEY=YourSecretKeyForJWTTokenGeneration123456789
JWT_ISSUER=SqlRestApi
JWT_AUDIENCE=SqlRestApi
JWT_EXPIRY_MINUTES=60

# Authentication Configuration
AUTH_USERNAME=admin
AUTH_PASSWORD=admin

# CORS Configuration (comma-separated origins)
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:5174,https://yourdomain.com

# Optional: Override other settings
# LOGGING_LEVEL=Information
# ALLOWED_HOSTS=*
